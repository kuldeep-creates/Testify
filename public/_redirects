# Netlify redirects for React Router
# This ensures that all routes are handled by the React app

# Serve static assets directly (don't redirect these)
/test/static/* /test/static/:splat 200
/test/favicon.ico /test/favicon.ico 200
/test/manifest.json /test/manifest.json 200
/test/logo192.png /test/logo192.png 200
/test/logo512.png /test/logo512.png 200

# Handle all other routes within the /test subdirectory
/test/* /test/index.html 200

# Fallback for root routes
/* /test/index.html 200
